    <script nonce="<%-nonce%>">
        // links and logout
        $(document).ready(function () {
            $('#pfPending').click(function () {
                window.location.href = '/dashboard/orders/pending';
            });
            $('#pfCompleted').click(function () {
                window.location.href = '/dashboard/orders/completed';
            });
            $('#pfCancelled').click(function () {
                window.location.href = '/dashboard/orders/cancelled';
            });
            $('#pfLifeTime').click(function () {
                window.location.href = '/dashboard/orders/lifetime';
            });

            $('#pfUsers').click(function () {
                window.location.href = '/dashboard/users';
            });

            $('#pfComplaint').click(function () {
                window.location.href = '/dashboard/cf/complaint';
            });

            $('#pfFeedback').click(function () {
                window.location.href = '/dashboard/cf/feedback';
            });

            $('#lgbtn1').click(async function () {
                await logout();
            });
            $('#navLogout').click(async function () {
                await logout();
            });

            async function logout() {
                document.getElementById("loader_container").style.display = "flex";

                try {
                    const response = await fetch('/dashboard/logout', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                    });

                    const result = await response.json();

                    if (result.type == "positive") {
                        window.location.href = '/';
                    } else {
                        showAlert("Something went wrong. Please try again.", "negative");
                        document.getElementById("loader_container").style.display = "none";
                    }
                } catch (error) {
                    console.error('Error:', error);
                    document.getElementById("loader_container").style.display = "none";
                    showAlert('Something went wrong. Please try again.', "negative");
                }
            }

            //live update data
            const socket = io("https://mintospeed.in", {
                transports: ["websocket"],
                withCredentials: true
            });

            // const socket = io("https://mintospeed.in", { transports: ["websocket"] });

            socket.on("ping", (data) => {
                console.log(data.message);
            });

            socket.on("updateMetaData", (metaData) => {
                console.log("ðŸ”¥ Live Meta Data:", metaData);
                showAlert('New data found.', "positive");

                // Function to update text with effect if changed
                const updateElement = (id, newValue) => {
                    const element = document.getElementById(id);
                    if (element && element.textContent != newValue) {
                        element.textContent = newValue;
                        element.classList.add("highlight"); // Add highlight effect

                        // Remove effect after animation duration
                        setTimeout(() => element.classList.remove("highlight"), 60000);
                    }
                };
            
                updateElement("totalPendingOrders", metaData.totalPendingOrders);
                updateElement("totalCompletedOrders", metaData.totalCompletedOrders);
                updateElement("totalCancelledOrders", metaData.totalCancelledOrders);
                updateElement("lifeTimeOrders", metaData.lifeTimeOrders);
                updateElement("totalComplaints", metaData.totalComplaints);
                updateElement("totalFeedbacks", metaData.totalFeedbacks);
            });
        });
    </script>

